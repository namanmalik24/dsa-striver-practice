#include <iostream>
#include<vector>
#include<string>
using namespace std;

//all subsequences will be printed

void printt(int i, vector<int>&ds, int n, vector<int>&arr)
{
    if(i==n)
    {
        for(auto it: ds)
        {
            cout<<it;
        }
        cout<<endl;
        return;
    }
    
    ds.push_back(arr[i]);
    printt(i+1,ds,n,arr);
    ds.pop_back();
    printt(i+1,ds,n,arr);
    
}

int main() {
    
    vector<int> ds;
    vector<int> arr = {1,2,3};
    int n =arr.size();
    
    printt(0,ds,n,arr);
    //last is empty ds printed.
    cout<<"finished";
    
    return 0;
}