#include <iostream>
#include <string>
using namespace std;

void fun(int i,int n)
{
    if(i>n)
    {
        return;
    }
    
    cout<<i<<endl;
    fun(i+1,n);
  
}

void fun2(int n)
{
    if(n==0)
    {
        return;
    }

    fun2(n-1);
    cout<<n<<endl;
}

int main() {
    
    fun(1,5);

    cout<<endl<<endl<<endl;

    fun2(5);

    return 0;
}
